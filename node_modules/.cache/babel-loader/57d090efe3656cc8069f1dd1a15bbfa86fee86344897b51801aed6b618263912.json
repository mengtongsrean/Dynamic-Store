{"ast":null,"code":"import ProductCard from '@/components/ProductCard.vue';\nimport Footer from '@/components/Footer.vue';\nexport default {\n  props: ['inventory', 'addToCart'],\n  components: {\n    ProductCard,\n    Footer\n  }\n};","map":{"version":3,"names":["ProductCard","Footer","props","components"],"sources":["/Users/macbookpro/Documents/University/Year 3/Pathway 1/COS30043 Interface Design and Development/HD Project/football-store-app/src/views/HomePage.vue"],"sourcesContent":["<template>\n  <section id=\"hero\" class=\"hero d-flex align-items-center section-bg\">\n    <div class=\"container\">\n      <div class=\"row justify-content-between gy-5\">\n        <div class=\"col-lg-5 order-2 order-lg-1 d-flex flex-column justify-content-center align-items-center align-items-lg-start text-center text-lg-start\">\n          <h2>Welcome to Dynamic</h2>\n          <p>Sed autem laudantium dolores. Voluptatem itaque ea consequatur eveniet. Eum quas beatae cumque eum quaerat.</p>\n          <div>\n            <router-link to=\"/products\" class=\"check-products\">See Products</router-link>\n          </div>\n        </div>\n        <div class=\"col-lg-5 order-1 order-lg-2 text-center text-lg-start\">\n          <!-- <img src=\"@/assets/images/jersey1.jpg\" class=\"img-fluid\" alt=\"\" data-aos=\"zoom-out\" data-aos-delay=\"300\"> -->\n          <div id=\"carouselExampleCaptions\" class=\"carousel slide carousel-fade\" data-bs-ride=\"carousel\">\n            <div class=\"carousel-indicators\">\n              <button type=\"button\" data-bs-target=\"#carouselExampleCaptions\" data-bs-slide-to=\"0\" class=\"active\" aria-current=\"true\" aria-label=\"Slide 1\"></button>\n              <button type=\"button\" data-bs-target=\"#carouselExampleCaptions\" data-bs-slide-to=\"1\" aria-label=\"Slide 2\"></button>\n              <button type=\"button\" data-bs-target=\"#carouselExampleCaptions\" data-bs-slide-to=\"2\" aria-label=\"Slide 3\"></button>\n            </div>\n            <div class=\"carousel-inner\">\n              <div class=\"carousel-item active\">\n                <img src=\"@/assets/images/slide1.jpg\" class=\"d-block w-100 img-fluid\" alt=\"slide-image1\">\n              </div>\n              <div class=\"carousel-item\">\n                <img src=\"@/assets/images/slide2.jpg\" class=\"d-block w-100 img-fluid\" alt=\"slide-image2\">\n              </div>\n              <div class=\"carousel-item\">\n                <img src=\"@/assets/images/slide3.jpg\" class=\"d-block w-100 img-fluid\" alt=\"slide-image3\">\n              </div>\n            </div>\n            <button class=\"carousel-control-prev\" type=\"button\" data-bs-target=\"#carouselExampleCaptions\" data-bs-slide=\"prev\">\n              <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\n              <span class=\"visually-hidden\">Previous</span>\n            </button>\n            <button class=\"carousel-control-next\" type=\"button\" data-bs-target=\"#carouselExampleCaptions\" data-bs-slide=\"next\">\n              <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\n              <span class=\"visually-hidden\">Next</span>\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section><!-- End Hero Section -->\n\n  <section style=\"background-color:eee\">\n    <div class=\"container py-5\">\n      <div class=\"row\">\n        <h2 class=\"text-center pb-5\">Recommended Products</h2>\n      </div>\n      <div class=\"row justify-content-center\">\n        <ProductCard\n        v-for=\"(product, index) in inventory.slice(0,3)\"\n        :key=\"product.id\"\n        :index=\"index\"\n        :product=\"product\"\n        :addToCart=\"addToCart\">\n      </ProductCard>\n      </div>\n    </div>\n  </section>\n  <Footer></Footer>\n</template>\n\n<script>\nimport ProductCard from '@/components/ProductCard.vue'\nimport Footer from '@/components/Footer.vue';\nexport default {\n  props: ['inventory', 'addToCart'],\n  components: {\n    ProductCard,\n    Footer\n  }\n}\n</script>\n\n<style>\n\n.section-bg {\n  background-color: #eee;\n}\n\n.hero {\n  width: 100%;\n  background-size: cover;\n  position: relative;\n  min-height: 50vh;\n  padding: 60px 0 60px 0;\n}\n\n.hero h2 {\n  font-size: 64px;\n  font-weight: 700;\n  margin-bottom: 20px;\n  color: var(--color-secondary);\n  font-family: var(--font-primary);\n}\n\n.hero h2 span {\n  color: var(--color-primary);\n}\n\n.hero p {\n  color: #4f4f5a;\n  font-weight: 400;\n  margin-bottom: 30px;\n}\n\n\n.hero .check-products {\n  font-weight: 500;\n  font-size: 14px;\n  letter-spacing: 1px;\n  padding: 12px 36px;\n  display: inline-block;\n  border-radius: 50px;\n  transition: 0.5s;\n  color: #ffffff; /* Text color */\n  background: #dc143c; /* Crimson background color */\n  box-shadow: 0 8px 28px rgba(220, 20, 60, 0.2); /* Crimson shadow */\n}\n\n.hero .check-products:hover {\n  background: #ff7f50; /* Orange background color */\n  box-shadow: 0 8px 28px rgba(255, 127, 80, 0.2); /* Orange shadow */\n}\n\n@media (max-width: 640px) {\n  .hero h2 {\n    font-size: 36px;\n  }\n  .hero .btn-get-started,\n  .hero .btn-watch-video {\n    font-size: 14px;\n  }\n}\n</style>"],"mappings":"AAgEA,OAAOA,WAAU,MAAO,8BAA6B;AACrD,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,eAAe;EACbC,KAAK,EAAE,CAAC,WAAW,EAAE,WAAW,CAAC;EACjCC,UAAU,EAAE;IACVH,WAAW;IACXC;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}