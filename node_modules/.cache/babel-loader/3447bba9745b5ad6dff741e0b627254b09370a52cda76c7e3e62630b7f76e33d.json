     {{ buttonLabel }}\n        </button>\n      </div>\n      <div v-else>\n        <button class=\"btn btn-primary\" @click=\"addToCart\">{{ buttonLabel }}</button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"ProductCard\",\n  props: {\n    name: { type: String, required: true },\n    price: { type: String, required: true },\n    unit: { type: String, required: true },\n    image: { type: String, required: true },\n    buttonLabel: { type: String, default: \"Add to cart\" },\n    showQuantity: { type: Boolean, default: true },\n    maxQuantity: { type: Number, default: 100 },\n  },\n  data() {\n    return {\n      quantity: 1,\n    };\n  },\n  methods: {\n    addToCart() {\n      // Emit an event for adding to cart with the product data\n      this.$emit(\"add-to-cart\", {\n        name: this.name,\n        quantity: this.quantity,\n        unit: this.unit,\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.product-card {\n  width: 18rem;\n  margin: 1rem;\n}\n\n.price {\n  font-size: 1.2em;\n  font-weight: bold;\n}\n\n.unit {\n  font-size: 0.9em;\n  color: gray;\n}\n\n.input-group input {\n  max-width: 3rem;\n}\n</style>\n"],"mappings":"AAiGA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLD,IAAI,EAAE;MAAEE,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACtCC,KAAK,EAAE;MAAEH,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACvCE,IAAI,EAAE;MAAEJ,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACtCG,KAAK,EAAE;MAAEL,IAAI,EAAEC,MAAM;MAAEC,QAAQ,EAAE;IAAK,CAAC;IACvCI,WAAW,EAAE;MAAEN,IAAI,EAAEC,MAAM;MAAEM,OAAO,EAAE;IAAc,CAAC;IACrDC,YAAY,EAAE;MAAER,IAAI,EAAES,OAAO;MAAEF,OAAO,EAAE;IAAK,CAAC;IAC9CG,WAAW,EAAE;MAAEV,IAAI,EAAEW,MAAM;MAAEJ,OAAO,EAAE;IAAI;EAC5C,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV;MACA,IAAI,CAACC,KAAK,CAAC,aAAa,EAAE;QACxBlB,IAAI,EAAE,IAAI,CAACA,IAAI;QACfe,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBT,IAAI,EAAE,IAAI,CAACA;MACb,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ign